    "react-dom": "^18.3.1",
    "reading-time": "^1.5.0",
    "remark-gfm": "^4.0.0",
    "schema-dts": "^1.1.5"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^0.5.10",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.28",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
